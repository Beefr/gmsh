# OS
UNAME=@UNAME@

# The names of the C and C++ compilers and linker
CC=@CC@
CXX=@CXX@
LINKER=@LINKER@

# Compiler flags
FLAGS=@FLAGS@ @INCLS@
OPTIM=@OPTIM@

# Gmsh subdirectories and libraries
GMSH_DIRS=@GMSH_DIRS@
GMSH_LIBS=@GMSH_LIBS@

# How you create a static library on this machine
AR=@AR@
RANLIB=@RANLIB@

# The extension to use for executables...
EXEEXT=@EXEEXT@

# Any operation to perform on the executable after the link
POSTBUILD=@POSTBUILD@

# Installation directories
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
datadir=@datadir@
includedir=@includedir@
libdir=@libdir@
mandir=@mandir@
infodir=@infodir@
